Tought = (Space v It)
Sought = (Existent v It)
Tought v Sought = (Space v It) v (Existent v It)
Tought ^ Sought = (Space v It) ^ (Existent v It)
Tought v CoSought = ((Space v It) v (Existent v It)) v ((Space ^ It) v (Existent v It))
Tought ^ CoSought = ((Space ^ It) v (Existent v It)) v ((Space v It) v (Existent ^ It))
