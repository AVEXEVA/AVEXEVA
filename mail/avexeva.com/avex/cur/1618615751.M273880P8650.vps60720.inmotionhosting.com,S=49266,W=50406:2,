                .appl_locker_label,=20
                .appl_locker_caption,=20
                .appl_locker_cta,
                .appl_story_link=20
                 {
                    font-size: 16px
                }

                .appl_story,
                .appl_story_last {
                    margin: 0 auto 35px !important;
                    text-align: center
                }
                .appl_story_media {
                    width: 100%
                }
                .appl_story .appl_mod_copy {
                    color: #333333;
                }

                .appl_module_locker_cell {
                    padding: 0 20px 20px;
                }

                .appl_legal {
                    box-sizing: border-box;
                    padding: 0 5px 10px;
                }

                /* Targets Gmail */
                u~div .full-wrap {
                    min-width: 100vw;
                }

                /* Targets Gmail on Android */
                div>u~div .full-wrap {
                    min-width: 100%;
                }

                .appl_desktop {
                    display: none !important;
                    margin: 0 auto;
                }

                .appl_mobile {
                    display: block !important;
                }
            }

            @media screen yahoo and (max-width:480px){
                .appl_module_wide {
                    height: 100px;
                }
                .appl_yahoo {
                    height: 1px;
                    width: 1px;
                }
                .appl_top_hero {
                    height: auto;
                }
                .appl_page_header {
                    margin-top: 30px;
                }
                .appl_footer {
                    padding: 10px 5px 0;
                    font-size: 11px;
                }
            }
